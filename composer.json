{
    "name": "umirode/spiral-json-rpc-server",
    "type": "library",
    "license": "MIT",
    "description": "Bridge for use Laminas JSON RPC with Spiral",
    "authors": [
        {
            "name": "umirode",
            "email": "mr.droiderm@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "laminas/laminas-json-server": "^3.2",
        "psr/http-server-handler": "^1.0",
        "spiral/boot": "^1.2",
        "psr/container": "^1.0",
        "laminas/laminas-diactoros": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "spiral/code-style": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Umirode\\JsonRpcServer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Umirode\\JsonRpcServer\\Tests\\": "tests/JsonRpcServer"
        }
    }
}
